{
  "name": "dapps-oauth-provider",
  "version": "0.0.1",
  "description": "OAuth2 provider for DApps",
  "main": "src/index.ts",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "nodemon src/docs/swagger.ts",
    "dev:debug": "nodemon --inspect src/index.ts",
    "lint": "eslint -c .eslintrc.json src/**/*.ts",
    "build": "tsc",
    "migration": "typeorm-ts-node-commonjs migration:generate -d ./src/db/data-source.ts",
    "migration-run": "typeorm-ts-node-commonjs migration:run -d ./src/db/data-source.ts",
    "debug": "export DEBUG=* && npm start",
    "swagger": "node -r ts-node/register ./src/docs/swagger.ts",
    "test": "mocha -r ts-node/register 'test/**/*.test.ts'",
    "test-debug": "export DEBUG=* && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaganathanb/dapps-oauth-provider.git"
  },
  "keywords": [
    "dapps",
    "rest-api",
    "oauth2"
  ],
  "author": "jaganathanb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaganathanb/dapps-oauth-provider/issues"
  },
  "homepage": "https://github.com/jaganathanb/dapps-oauth-provider#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-oauth-server": "^2.0.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "express-winston": "^4.2.0",
    "helmet": "^7.1.0",
    "http-status": "^1.7.3",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.4",
    "oauth2-server": "^3.1.1",
    "reflect-metadata": "^0.2.1",
    "sqlite3": "5.1.7-rc.0",
    "typeorm": "^0.3.17",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^4.3.11",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/mocha": "^10.0.6",
    "@types/morgan": "^1.9.9",
    "@types/oauth2-server": "^3.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "chai": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i": "^2.29.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^47.0.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "pretty-quick": "^3.1.3",
    "swagger-autogen": "^2.23.7",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
